procedure triangle (first : real, second : real, third : real)
    if first + second + third not= 180 then
	put "The angles are not equal to 180."
    end if
    if first = second and second = third then
	put "Your triangle is a equilateral triangle."
    elsif first = second or second = third or third = first then
	put "Your triangle is an isosceles triangle."
    else
	put "Your triangle is scalene."
    end if
end triangle

var angle, angle2, angle3 : int
put "Input the three angles of your triangle." ..
get angle, angle2, angle3
triangle (angle, angle2, angle3)


